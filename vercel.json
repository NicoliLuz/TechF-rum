{
  "version": 2,
  "builds": [
    {
      "src": "back/src/server.js",
      "use": "@vercel/node",
      "outputDirectory": "back"
    },
    {
      "src": "front/**",
      "use": "@vercel/static",
      "outputDirectory": "front"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "back/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/front/$1"
    }
  ]
}